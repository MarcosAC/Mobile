{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Android",
        "type": "reactnative",
        "request": "launch",
        "platform": "android",
        "cwd": "${workspaceFolder}",
        "sourceMaps": true,
        "outDir": "${workspaceFolder}/.vscode/.react"
      },
      {
        "name": "Attach to packager",
        "type": "reactnative",
        "request": "attach",
        "cwd": "${workspaceFolder}",
        "port": 8081,
        "sourceMaps": true,
        "outDir": "${workspaceFolder}/.vscode/.react"
      }
      
    ]
  }
  